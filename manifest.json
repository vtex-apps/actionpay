{
  "name": "actionpay",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "Actionpay",
  "description": "Actionpay VTEX App",
  "billingOptions": {
    "termsURL": "",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "settingsSchema": {
    "title": "Actionpay Settings",
    "type": "object",
    "properties": {
      "actionpayPartnerId": {
        "title": "Partner ID",
        "description": "Enter your Actionpay Partner ID",
        "type": "string"
      },
      "actionpayDomain": {
        "title": "Domain",
        "description": "Enter your Actionpay Domain e.g: example.com (without \"www.\")",
        "type": "string"
      },
      "actionpayPostclick": {
        "title": "Post-click",
        "description": "Enter your Actionpay Post-click",
        "type": "number"
      },
      "actionpayCampaignId": {
        "title": "Campaign ID",
        "description": "Enter your Actionpay Campaign ID",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
